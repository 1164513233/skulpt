{
  "name": "skulpt",
  "version": "1.0.0",
  "description": "Skulpt is a Javascript implementation of Python 2.x. Python that runs in your browser!",
  "keywords": [
    "skulpt"
  ],
  "scripts": {
    "regensymtabtests": "python skulpt.py regensymtabtests",
    "vmwareregr": "python skulpt.py vmwareregr",
    "regenparser": "python skulpt.py regenparser",
    "regenasttests": "python skulpt.py regenasttests",
    "regenruntests": "python skulpt.py regenruntests",
    "upload": "python skulpt.py upload",
    "doctest": "python skulpt.py doctest",
    "browser": "python skulpt.py browser",
    "debugbrowser": "python skulpt.py debugbrowser",
    "vfs": "python skulpt.py vfs",
    "host": "python skulpt.py host",
    "doc": "echo 'Building Documentation in docs/ProgMan' && jsdoc -c jsdoc.json HACKING.md",
    "dist": "npm run build && npm run testopt && npm run test3opt && npm run doc && node support/build/copy2docs.js",
    "repl": "npm run devbuild && node repl/repl.js dist/skulpt.js",
    "prebuild": "node support/build/wrapmodules.js internal",
    "build": "webpack --mode production",
    "postbuild": "node support/build/wrapmodules.js builtin",
    "predevbuild": "node support/build/wrapmodules.js internal",
    "devbuild": "webpack --mode development",
    "postdevbuild": "node support/build/wrapmodules.js builtin",
    "test": "node test/testwrapper.js skulpt.js && node test/testunit.js skulpt.js python2",
    "testopt": "node test/testwrapper.js skulpt.min.js && node test/testunit.js skulpt.min.js python2",
    "test3": "node test/testunit.js skulpt.js python3",
    "test3opt": "node test/testunit.js skulpt.min.js python3",
    "run": "node test/runfile.js skulpt.js python2",
    "run3": "node test/runfile.js skulpt.js python3",
    "runopt": "node test/runfile.js skulpt.min.js python2",
    "run3opt": "node test/runfile.js skulpt.min.js python3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/skulpt/skulpt.git"
  },
  "bugs": {
    "url": "https://github.com/skulpt/skulpt/issues"
  },
  "homepage": "http://www.skulpt.org/",
  "author": {
    "name": "Brad Miller"
  },
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "chalk": "^2.4.2",
    "clean-webpack-plugin": "^2.0.2",
    "closure-webpack-plugin": "^2.0.1",
    "copy-webpack-plugin": "^5.0.3",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.1.2",
    "git-revision-webpack-plugin": "^3.0.3",
    "google-closure-compiler": "^20190415.0.0",
    "js-beautify": "^1.10.0",
    "jsdoc": "~3.5.5",
    "jshint": "^2.5.11",
    "micro-strptime": "^0.2.3",
    "readline-sync": "^1.4.9",
    "setimmediate": "^1.0.5",
    "shelljs": "^0.8.3",
    "strftime": "^0.10.0",
    "webpack": "^4.32.0",
    "webpack-cli": "^3.3.2",
    "webpack-jshint-loader": "^0.9.1"
  },
  "engines": {
    "node": ">=8.10"
  },
  "dependencies": {}
}
